{
  "name": "kingland-royale-monorepo",
  "private": true,
  "version": "0.1.0",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "npm-run-all -p dev:server dev:client",
    "dev:server": "npm --workspace @kingland/server run dev",
    "dev:client": "npm --workspace @kingland/client run dev",
    "build": "npm --workspace @kingland/shared run build && npm --workspace @kingland/server run build && npm --workspace @kingland/client run build",
    "start": "npm --workspace @kingland/server run start"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}